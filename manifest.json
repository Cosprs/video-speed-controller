{
  "name": "VideoSpeed",
  "version": "1.0",
  "description": "Speed up Netflix, Youtube, etc.",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo.png",
      "32": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    }
  },
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "manifest_version": 2
}
